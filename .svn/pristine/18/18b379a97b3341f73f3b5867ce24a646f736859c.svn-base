header{
	width: 100%;
	height: 40px;
	background: #f2f2f2;
}
.header-box{
	/*background: bisque;*/
	width: 1000px;
	height: 34px;
	margin: 0 auto;
}
.header-box span {
	padding-left: 6px;
   line-height: 34px;
   height: 34px;
  display: inline-block;
	
	/*text-align: center;*/
}
.serach-box{
	
   padding-top: 10px;
	width: 1000px;
	margin: 0 auto;
	height: 80px;
	/*background: darkblue;*/
}
.serach-box-left{
	padding: 10px 6px ;
}
.serach-box-right{
	padding: 10px 6px ;
	margin-right: 10px;
}
.serach-box input[type='text']{
	height: 30px;
	width: 500px;
	border: 1px solid darkmagenta;
}
.serach-box input[type='button']{
	height: 32px;
	width: 50px;
	background: darkmagenta;
	color: white;
	border:none;
}